\end{picture}}
\put(-\value{x},\value{y}){\circle{\value{z}}}%
\put(\value{x},-\value{y}){\circle{\value{z}}}%
\truex{300}\truey{1000}\truez{600}%
\put(-\value{x},\value{x}){\makebox(0,\value{z})[r]{${#1}$}}%
\put(\value{x},-\value{y}){\makebox(0,\value{z})[l]{${#2}$}}%
\end{picture}}%

