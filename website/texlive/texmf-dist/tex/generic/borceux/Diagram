\multiply\xstep by 9486%
\divide\xstep by 10000%
\ystep=\xstep%
\divide\ystep by 3%
\makehead{\xstep}{\ystep}{\numberofsteps}}
\newcommand{\nnehead}{\usebox{\northnortheasthead}}

\newsavebox{\southsouthwesthead}
\savebox{\southsouthwesthead}{%
