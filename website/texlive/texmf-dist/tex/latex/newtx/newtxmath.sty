   \def\@FutureLetDecide{%
     \ifx\@nil\@FutureLetToken
        \def\@FutureLetNext{\@EachCharacterDone}
     \else
        \def\@FutureLetNext{\@PickUpTheCharacter}
     \fi
     \@FutureLetNext
   }%
