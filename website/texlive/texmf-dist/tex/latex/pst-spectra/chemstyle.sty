  \ifcst@radsuper
    \let\cst@radical@super\textsuperscript
  \fi
  \ifcst@radprefix
    \expandafter\cst@radical@pre
  \else
    \expandafter\cst@radical@post
  \fi
}
\newcommand*\cst@radical@pre[2]{%
