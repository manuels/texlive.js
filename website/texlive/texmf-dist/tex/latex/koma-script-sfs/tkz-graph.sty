\Edge[style={post,bend right=20},label={$\y$}](grB)(grA)
\Edge[style={post,bend right=20},label={$\z$}](grB)(grC)}
   \foreach \x/\y/\z in {#7} {%
   \grLoop[dist=\cmdGR@grpb@LoopDist,dir=SO,label={$\x$}](grC)
 \Edge[style={post,bend right=20},label={$\y$}](grC)(grA)
