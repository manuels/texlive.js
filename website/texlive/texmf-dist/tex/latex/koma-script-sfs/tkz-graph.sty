   \Vertex[Node,Lpos=\cmdGR@grpb@LposA,Ldist=\cmdGR@grpb@Ldist,L=#4]{grC}
   \foreach \x/\y/\z in {#5} {%
   \grLoop[dist=\cmdGR@grpb@LoopDist,dir=EA,label={$\x$}](grA)
\Edge[style={post,bend right=20},label={$\y$}](grA)(grB)
