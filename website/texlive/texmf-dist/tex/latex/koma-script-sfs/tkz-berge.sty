\newcommand*{\EdgeInGraphModLoop}[5][]{% #2 order
\begingroup%
  \setkeys[GR]{edge}{#1}%
    \setcounter{tkz@gr@ta}{#5}
    \setcounter{tkz@gr@n}{#3}
