\newcommand*{\EdgeDoubleMod}[9]{%
\begingroup%
   \foreach \V@x in {0,...,#9}{%
      \pgfmathsetcounter{tkz@gr@c}{Mod(#3+(#4*\V@x),#2)}
      \pgfmathsetcounter{tkz@gr@n}{Mod(#7+(#8*\V@x),#6)}
