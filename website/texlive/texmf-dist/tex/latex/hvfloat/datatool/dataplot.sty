  \edef\dtl@legendline{\the\@dtl@toks\the\toks@}%
\fi
\expandafter\toks@\expandafter{\dtl@legendline}%
\ifx\dtl@legend\@empty
  \edef\dtl@legend{\noexpand\tikz\the\toks@; \noexpand& #3}%
\else
