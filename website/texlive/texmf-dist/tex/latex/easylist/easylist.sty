\def\el@UnitSearch#1{%
\let\easynext\el@UnitSearch
\if#1-%
	\ifSign
		\DubiousFiguretrue
		\def\easynext##1?{\relax}%
	\else
		\Signtrue
	\fi
\else
	\if#1+%
		\ifSign
			\DubiousFiguretrue
			\def\easynext##1?{\relax}%
		\else
			\Signtrue
		\fi
	\else
	\Signtrue
		\if#1?%
			\DubiousFiguretrue
			\let\easynext\relax
		\else
			\if#1.%
				\ifPoint
					\DubiousFiguretrue
					\def\easynext##1?{\relax}%
				\else
					\Pointtrue
				\fi
			\else
				\if#1,%
					\ifPoint
						\DubiousFiguretrue
						\def\easynext##1?{\relax}%
					\else
						\Pointtrue
					\fi
				\else
					\ifcat#1a%
						\def\easynext{\el@UnitCheck#1}%
					\else
						\el@NumberCheck#1?%
						\ifDubiousFigure
							\def\easynext{\el@DimenUnitCheck#1}%
						\fi
					\fi
				\fi
			\fi
		\fi
	\fi
\fi\easynext}

