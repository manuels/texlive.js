      \multiply \c@nprt@cntprint 3%
      \advance\c@nprt@digitsfirstblock by -\thenprt@cntprint\relax
      \ifnum\thenprt@digitsfirstblock=1
        \expandafter\nprt@printone\nprt@numbertoprint\@empty
