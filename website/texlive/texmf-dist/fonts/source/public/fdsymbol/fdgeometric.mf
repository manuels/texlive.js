  theta := angle(z1 - z0);

  x0' = x0 + line_thickness / sind theta;
  x2' = x2 - line_thickness / sind theta;
  y0' = y2' = y0;
  y1' = y1 - line_thickness / cosd theta;
  y3' = y3 + line_thickness / cosd theta;
  x1' = x3' = x1;

  fill z0 -- z1 -- z2 -- z3 -- cycle;
  unfill z0' -- z1' -- z2' -- z3' -- cycle;
endchar;

